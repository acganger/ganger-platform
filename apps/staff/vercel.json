{
  "ignoreCommand": "cd ../.. && npx turbo-ignore @ganger/staff",
  "installCommand": "cd ../.. && npm install && npm run build:auth && npm run build:db && npm run build:integrations && npm run build:types && npm run build:ui && npm run build:utils && npm run build:config",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "development"
  },
  "rewrites": [
    {
      "source": "/inventory/:path*",
      "destination": "https://ganger-inventory-ganger.vercel.app/inventory/:path*"
    },
    {
      "source": "/handouts/:path*",
      "destination": "https://ganger-handouts-ganger.vercel.app/handouts/:path*"
    },
    {
      "source": "/compliance/:path*",
      "destination": "https://ganger-compliance-training-ganger.vercel.app/compliance/:path*"
    },
    {
      "source": "/clinical-staffing/:path*",
      "destination": "https://ganger-clinical-staffing-ganger.vercel.app/clinical-staffing/:path*"
    },
    {
      "source": "/config/:path*",
      "destination": "https://ganger-config-dashboard-ganger.vercel.app/config/:path*"
    },
    {
      "source": "/kiosk/:path*",
      "destination": "https://ganger-checkin-kiosk-ganger.vercel.app/kiosk/:path*"
    },
    {
      "source": "/platform-dashboard/:path*",
      "destination": "https://ganger-platform-dashboard-ganger.vercel.app/platform-dashboard/:path*"
    },
    {
      "source": "/l10/:path*",
      "destination": "/coming-soon?app=eos-l10"
    },
    {
      "source": "/batch/:path*",
      "destination": "/coming-soon?app=batch-closeout"
    },
    {
      "source": "/status/:path*",
      "destination": "/coming-soon?app=integration-status"
    },
    {
      "source": "/pharma/:path*",
      "destination": "/coming-soon?app=pharma-scheduling"
    },
    {
      "source": "/lunch/:path*",
      "destination": "/coming-soon?app=pharma-scheduling"
    },
    {
      "source": "/socials/:path*",
      "destination": "/coming-soon?app=socials-reviews"
    },
    {
      "source": "/ai-receptionist/:path*",
      "destination": "/coming-soon?app=ai-receptionist"
    },
    {
      "source": "/call-center/:path*",
      "destination": "/coming-soon?app=call-center-ops"
    },
    {
      "source": "/medication-auth/:path*",
      "destination": "/coming-soon?app=medication-auth"
    },
    {
      "source": "/components/:path*",
      "destination": "/coming-soon?app=component-showcase"
    }
  ]
}
