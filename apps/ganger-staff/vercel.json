{
  "installCommand": "cd ../.. && pnpm install --no-frozen-lockfile && pnpm -F @ganger/auth build && pnpm -F @ganger/ui build && pnpm -F @ganger/utils build && pnpm -F @ganger/config build",
  "buildCommand": "cd ../.. && pnpm -F @ganger/staff build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
