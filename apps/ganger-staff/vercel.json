{
  "installCommand": "cd ../.. && NODE_ENV=development pnpm install --no-frozen-lockfile && pnpm -F @ganger/auth build && pnpm -F @ganger/ui build && pnpm -F @ganger/utils build && pnpm -F @ganger/config build",
  "buildCommand": "cd ../.. && pnpm -F @ganger/staff build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/inventory/:path*", "destination": "https://ganger-inventory.vercel.app/:path*" },
    { "source": "/actions/:path*", "destination": "https://ganger-actions.vercel.app/:path*" },
    { "source": "/handouts/:path*", "destination": "https://ganger-handouts.vercel.app/:path*" },
    { "source": "/l10/:path*", "destination": "https://ganger-eos-l10.vercel.app/:path*" },
    { "source": "/eos-l10/:path*", "destination": "https://ganger-eos-l10.vercel.app/:path*" },
    { "source": "/batch/:path*", "destination": "https://ganger-batch-closeout.vercel.app/:path*" },
    { "source": "/batch-closeout/:path*", "destination": "https://ganger-batch-closeout.vercel.app/:path*" },
    { "source": "/compliance/:path*", "destination": "https://ganger-compliance-training.vercel.app/:path*" },
    { "source": "/compliance-training/:path*", "destination": "https://ganger-compliance-training.vercel.app/:path*" },
    { "source": "/clinical-staffing/:path*", "destination": "https://ganger-clinical-staffing.vercel.app/:path*" },
    { "source": "/staffing/:path*", "destination": "https://ganger-clinical-staffing.vercel.app/:path*" },
    { "source": "/config/:path*", "destination": "https://ganger-config-dashboard.vercel.app/:path*" },
    { "source": "/config-dashboard/:path*", "destination": "https://ganger-config-dashboard.vercel.app/:path*" },
    { "source": "/status/:path*", "destination": "https://ganger-integration-status.vercel.app/:path*" },
    { "source": "/integration-status/:path*", "destination": "https://ganger-integration-status.vercel.app/:path*" },
    { "source": "/ai-receptionist/:path*", "destination": "https://ganger-ai-receptionist.vercel.app/:path*" },
    { "source": "/call-center/:path*", "destination": "https://ganger-call-center-ops.vercel.app/:path*" },
    { "source": "/call-center-ops/:path*", "destination": "https://ganger-call-center-ops.vercel.app/:path*" },
    { "source": "/medication-auth/:path*", "destination": "https://ganger-medication-auth.vercel.app/:path*" },
    { "source": "/pharma/:path*", "destination": "https://ganger-pharma-scheduling.vercel.app/:path*" },
    { "source": "/lunch/:path*", "destination": "https://ganger-pharma-scheduling.vercel.app/:path*" },
    { "source": "/pharma-scheduling/:path*", "destination": "https://ganger-pharma-scheduling.vercel.app/:path*" },
    { "source": "/kiosk/:path*", "destination": "https://ganger-checkin-kiosk.vercel.app/:path*" },
    { "source": "/checkin-kiosk/:path*", "destination": "https://ganger-checkin-kiosk.vercel.app/:path*" },
    { "source": "/socials/:path*", "destination": "https://ganger-socials-reviews.vercel.app/:path*" },
    { "source": "/socials-reviews/:path*", "destination": "https://ganger-socials-reviews.vercel.app/:path*" },
    { "source": "/components/:path*", "destination": "https://ganger-component-showcase.vercel.app/:path*" },
    { "source": "/component-showcase/:path*", "destination": "https://ganger-component-showcase.vercel.app/:path*" },
    { "source": "/platform/:path*", "destination": "https://ganger-platform-dashboard.vercel.app/:path*" },
    { "source": "/platform-dashboard/:path*", "destination": "https://ganger-platform-dashboard.vercel.app/:path*" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}