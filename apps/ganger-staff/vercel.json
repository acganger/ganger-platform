    },
    {
      "source": "/medication-auth/:path*",
      "destination": "https://ganger-medication-auth.vercel.app/medication-auth/:path*"
    },
    {
      "source": "/pharma/:path*",
      "destination": "https://ganger-pharma-scheduling.vercel.app/pharma/:path*"
    },
    {
      "source": "/lunch/:path*",
      "destination": "https://ganger-pharma-scheduling.vercel.app/pharma/:path*"
