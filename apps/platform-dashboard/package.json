{
  "name": "@ganger/platform-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Platform Entrypoint Dashboard - Central hub for Ganger Platform applications",
  "scripts": {
    "dev": "next dev -p 3011",
    "build": "next build && npx @cloudflare/next-on-pages",
    "start": "next start -p 3011",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "export": "next build && next export",
    "build:next": "next build",
    "build:worker": "npx @cloudflare/next-on-pages",
    "deploy": "npm run build && wrangler pages deploy .vercel/output/static --project-name=platform-dashboard-production"
  },
  "dependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@ganger/auth": "workspace:^",
    "@ganger/types": "workspace:^",
    "@ganger/ui": "workspace:^",
    "@heroicons/react": "^2.0.18",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.39.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.516.0",
    "next": "^14.2.0",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^9.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.13.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.5.0"
  },
  "keywords": [
    "dashboard",
    "platform",
    "ganger",
    "medical",
    "react",
    "nextjs"
  ]
