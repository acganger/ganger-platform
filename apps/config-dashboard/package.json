{
  "name": "@ganger/config-dashboard",
  "version": "1.0.0",
  "description": "Consolidated Configuration Dashboard for Ganger Platform",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3010",
    "build": "next build && npx @cloudflare/next-on-pages",
    "start": "next start -p 3010",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "build:next": "next build",
    "build:worker": "npx @cloudflare/next-on-pages",
    "deploy": "npm run build && wrangler pages deploy .vercel/output/static --project-name=config-dashboard-production"
  },
  "dependencies": {
    "@ganger/auth": "workspace:*",
    "@ganger/db": "workspace:*",
    "@ganger/ui": "workspace:*",
    "@ganger/utils": "workspace:*",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.38.4",
    "@tailwindcss/postcss": "^4.1.10",
    "next": "^14.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.0",
    "@cloudflare/next-on-pages": "^1.13.12"
  }
}