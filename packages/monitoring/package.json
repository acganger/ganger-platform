{
  "name": "@ganger/monitoring",
  "version": "1.0.0",
  "description": "Centralized monitoring and health check system for Ganger Platform",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": "./index.ts",
    "./sentry": "./sentry.ts",
    "./performance-tracking": "./performance-tracking.ts"
  },
  "scripts": {
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit -p tsconfig.build.json"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@ganger/db": "workspace:*",
    "@ganger/cache": "workspace:*",
    "@ganger/auth": "workspace:*",
    "typescript": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0"
  },
  "files": [
    "src",
    "index.ts",
    "sentry.ts",
    "performance-tracking.ts"
  ],
  "keywords": [
    "ganger-platform",
    "monitoring",
    "health-checks",
    "observability",
    "medical-platform"
  ]
