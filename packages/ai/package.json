{
  "name": "@ganger/ai",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./client": {
      "types": "./src/client/index.ts",
      "import": "./src/client/index.ts",
      "require": "./src/client/index.ts"
    },
    "./server": {
      "types": "./src/server/index.ts",
      "import": "./src/server/index.ts",
      "require": "./src/server/index.ts"
    },
    "./shared": {
      "types": "./src/shared/index.ts",
      "import": "./src/shared/index.ts",
      "require": "./src/shared/index.ts"
    },
    "./components": {
      "types": "./src/client/components.tsx",
      "import": "./src/client/components.tsx",
      "require": "./src/client/components.tsx"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@ganger/auth": "file:../auth",
    "@ganger/db": "file:../db",
    "@ganger/types": "file:../types",
    "@ganger/utils": "file:../utils",
    "@supabase/supabase-js": "^2.49.10",
    "zod": "^3.25.51",
    "date-fns": "^4.1.0",
    "uuid": "^11.0.3",
    "react": "^18.3.1",
    "next": "^14.2.29",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "next": "^14.0.0"
  }
}