{
  "name": "@ganger/ai",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.js",
      "require": "./dist/shared/index.js"
    },
    "./components": {
      "types": "./dist/client/components.d.ts",
      "import": "./dist/client/components.js",
      "require": "./dist/client/components.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@ganger/auth": "file:../auth",
    "@ganger/db": "file:../db",
    "@ganger/types": "file:../types",
    "@ganger/utils": "file:../utils",
    "@supabase/supabase-js": "^2.49.10",
    "zod": "^3.25.51",
    "date-fns": "^4.1.0",
    "uuid": "^11.0.3",
    "react": "^18.3.1",
    "next": "^14.2.29",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "next": "^14.0.0"
  }
}