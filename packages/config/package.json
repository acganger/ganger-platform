{
  "name": "@ganger/config",
  "version": "0.1.0",
  "description": "Shared configuration files for Ganger Platform",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "eslint/**/*",
    "typescript/**/*",
    "tailwind/**/*",
    "environment.*",
    "next-config-template.*",
    "supabase-template.*",
    "index.*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
    "check-types": "tsc --noEmit -p tsconfig.build.json"
  },
  "keywords": [
    "ganger-platform",
    "eslint",
    "typescript",
    "tailwind",
    "configuration",
    "environment",
    "next",
    "supabase"
  ],
  "author": "Ganger Platform Team",
  "license": "MIT",
  "dependencies": {
    "@ganger/db": "workspace:*",
    "@supabase/supabase-js": "^2.39.0",
    "typescript": "^5.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "next": "^14.2.0",
    "tailwindcss": "^3.4.0",
    "webpack-bundle-analyzer": "^4.10.0"
  },
  "peerDependencies": {
    "@ganger/db": "workspace:*",
    "next": ">=14.0.0"
  }
}