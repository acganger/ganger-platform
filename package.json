{
  "name": "ganger-platform",
  "version": "1.0.0",
  "private": true,
  "description": "Ganger Platform - A comprehensive platfor for Ganger Dermatology",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:all": "turbo run build",
    "build:shared": "turbo run build --filter='./packages/*'",
    "build:apps": "turbo run build --filter='./apps/*'",
    "build:ganger-actions": "turbo run build --filter=@ganger/ganger-actions",
    "build:staff": "turbo run build --filter=@ganger/ganger-staff",
    "build:changed": "turbo run build --filter='...[HEAD^1]'",
    "dev": "turbo run dev",
    "dev:ganger-actions": "turbo run dev --filter=@ganger/ganger-actions",
    "dev:staff": "turbo run dev --filter=@ganger/ganger-staff",
    "lint": "turbo run lint",
    "lint:all": "turbo run lint",
    "lint:changed": "turbo run lint --filter='...[HEAD^1]'",
    "test": "turbo run test",
    "test:all": "turbo run test",
    "test:changed": "turbo run test --filter='...[HEAD^1]'",
    "type-check": "turbo run type-check",
    "typecheck:all": "turbo run typecheck",
    "type-check:changed": "turbo run type-check --filter='...[HEAD^1]'",
    "verify": "turbo run verify",
    "verify:changed": "turbo run verify --filter='...[HEAD^1]'",
    "verify:all": "bash scripts/verify-deployment-ready.sh",
    "verify:app": "bash scripts/verify-deployment-ready.sh --app",
    "list:changed:apps": "git diff --name-only HEAD~1 | grep '^apps/' | cut -d'/' -f2 | sort -u | tr '\\n' ' '",
    "validate:builds": "bash scripts/validate-builds.sh",
    "validate:types": "bash scripts/validate-types.sh",
    "predeploy": "bash scripts/pre-deploy-check.sh",
    "deploy": "turbo run deploy",
    "deploy:changed": "turbo run deploy --filter='...[HEAD^1]'",
    "clean": "turbo run clean",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "turbo run db:studio",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "deploy:staging": "turbo run build && gcloud app deploy app-staging.yaml",
    "deploy:production": "turbo run build && gcloud app deploy app-production.yaml",
    "build:production": "turbo build",
    "deploy:check": "turbo build && turbo test",
    "validate": "npm run build:production && echo 'Ready for deployment'",
    "validate:env": "node scripts/validate-environment.js",
    "validate:env:production": "NODE_ENV=production node scripts/validate-environment.js",
    "docs:update": "node scripts/docs/update-docs.js",
    "docs:check": "bash scripts/docs/updatedocs.sh",
    "docs:sprint": "node scripts/docs/sprint.js",
    "dev:showcase": "cd apps/component-showcase && npm run dev",
    "build:packages": "turbo run build --filter=@ganger/auth --filter=@ganger/cache --filter=@ganger/config --filter=@ganger/db --filter=@ganger/integrations --filter=@ganger/monitoring --filter=@ganger/types --filter=@ganger/ui --filter=@ganger/utils",
    "ci:check": "pnpm install --frozen-lockfile && turbo run build lint test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "playwright:install": "playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "playwright": "^1.54.2",
    "prettier": "^3.1.0",
    "turbo": "^1.13.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.13.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.53.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "googleapis": "^154.1.0",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.536.0",
    "mysql2": "^3.14.3",
    "next": "^15.4.5",
    "nodemailer": "^7.0.5",
    "openai": "^5.11.0",
    "prisma": "^6.13.0",
    "puppeteer": "^24.15.0",
    "qrcode": "^1.5.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "stripe": "^18.4.0",
    "twilio": "^5.8.0",
    "zod": "^3.25.67"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.9",
      "@types/react-dom": "19.1.7",
      "react": "19.1.1",
      "react-dom": "19.1.1",
      "form-data": "^4.0.4",
      "jspdf": "^3.0.1"
    }
  }
}
